<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.silver5302.kakaologin2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/joinsoccer"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".IntroActivity"
            android:theme="@style/AppTheme.NoAction">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />

        <activity android:name=".MainActivity" />
        <activity android:name=".RegistMemberActivity" />
        <activity android:name=".ReadyListActivity" />
        <activity android:name=".MatchingActivity" />
        <activity android:name=".MatchingRegistActivity" />
        <activity android:name=".SelectRegionActivity" />
        <activity android:name=".MatchingInformActivity" />
        <activity android:name=".MatchPermissonActivity" />
        <activity android:name=".RegistMeceneryActivity"></activity>
    </application>

</manifest>